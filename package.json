{
  "name": "hubot-figlet",
  "version": "0.1.2",
  "description": "hubot generate figlet",
  "main": "index.coffee",
  "scripts": {
    "test": "mocha",
    "test-html-cov": "mocha -r blanket -R html-cov",
    "coveralls": "mocha -r blanket -R mocha-lcov-reporter | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mnpk/hubot-figlet.git"
  },
  "keywords": [
    "hubot",
    "figlet",
    "bot",
    "ascii"
  ],
  "author": "mnpk <mnncat@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mnpk/hubot-figlet/issues"
  },
  "homepage": "https://github.com/mnpk/hubot-figlet",
  "dependencies": {
    "figlet": "^1.1.0"
  },
  "devDependencies": {
    "blanket": "alex-seville/blanket",
    "chai": "^2.2.0",
    "coffee-script": "^1.9.2",
    "coveralls": "^2.11.2",
    "hubot": "~2.7.2",
    "hubot-mock-adapter": "^1.0.0",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2"
  },
  "config": {
    "blanket": {
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": "node_modules",
      "data-cover-only": "src"
    }
  }
}
